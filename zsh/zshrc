# Path to your oh-my-zsh installation.
export ZSH=/Users/grzegorz/.oh-my-zsh

ZSH_THEME="unijewski"

plugins=(git bundler osx rake ruby)

export PATH="$PATH:$HOME/.rvm/bin:/Users/grzegorz/.bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin"

source $ZSH/oh-my-zsh.sh

export BUNDLER_EDITOR="code --wait"
export EDITOR="code --wait"
export TERM=xterm-256color
export VIMRC="~/.vimrc"

alias dc=docker-compose

function gfr() {
  git reset @~ "$@" && git commit --amend --no-edit
}

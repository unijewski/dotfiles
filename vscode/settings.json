{
  "workbench.colorTheme": "Monokai",
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "vim.insertModeKeyBindings": [
      {
          "before": ["j", "k"],
          "after": ["<Esc>"]
      }
  ],
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "editor.rulers": [
      120
